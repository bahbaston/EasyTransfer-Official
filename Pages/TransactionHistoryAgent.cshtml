@page
@model easytransfer.Pages.TransactionListModel

<h1>Transaction History For Agent Only</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayName("Transaction id")
            </th>
            <th>
                @Html.DisplayName("Sender")
            </th>
            <th>
                @Html.DisplayName("Receiver")
            </th>
            <th>
                @Html.DisplayName("Tranaction date")
            </th>
            <th>
                @Html.DisplayName("Transaction type")
            </th>
            <th>
                @Html.DisplayName("Amount")
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.TransactionList)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.trhisid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.sender)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.receiver)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.transactiondate)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.transactiontype)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.amount)
                </td>


            </tr>
        }
    </tbody>
</table>
